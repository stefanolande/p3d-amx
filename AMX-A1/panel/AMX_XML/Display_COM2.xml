<SimBase.Document Type="AceXML" version="1,0">
<Filename>Display_COM2.xml</Filename>
<Descr>AceXML Document</Descr>
<SimGauge.Gauge id="Gauge">
<FloatPosition>0,0</FloatPosition>
<Size>150,30</Size>
<Update_When_Hidden>false</Update_When_Hidden>

<!-- For Lago AMX gauge - display Com freqs  left console -->
<Element id="Com2 Readout">
<FloatPosition>25,2</FloatPosition>
<Visibility>(A:CIRCUIT GENERAL PANEL ON, bool)</Visibility>
	<GaugeText id="Com2 Readout">
	<Size>150,30</Size>
	<Bright>True</Bright>
	<FontFace>Quartz</FontFace>
	<FontColor>#d58400</FontColor>
	<FontHeight>30</FontHeight>
	<Bold>False</Bold>
	<Transparent>true</Transparent>
	<HorizontalAlign>LEFT</HorizontalAlign>
	<GaugeString>%((A:COM ACTIVE FREQUENCY:1, MHz))%!3.3f!</GaugeString>
	</GaugeText>
</Element>

</SimGauge.Gauge>
</SimBase.Document>
